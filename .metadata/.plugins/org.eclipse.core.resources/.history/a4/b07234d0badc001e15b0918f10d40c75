#ifndef MAIN_H_
#define MAIN_H_

#include <stdint.h>

typdef struct
{
	uint32_t gpioAen	:1;
	uint32_t gpioBen	:1;
	uint32_t gpioCen	:1;
	uint32_t gpioDen	:1;
	uint32_t gpioEen	:1;
	uint32_t reserved	:2;
	uint32_t gpioHen	:1;
	uint32_t reserved1	:4;
	uint32_t crcen		:1;
	uint32_t reserved2	:8;
	uint32_t dma1en		:1;
	uint32_t dma2en		:1;
	uint32_t reserved3	:9;
}RCC_AHB1ENR_t;



#endif /* MAIN_H_ */
